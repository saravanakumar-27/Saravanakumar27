#include<stdio.h>
void main()
{
    int n,i,arr[100];
    scanf("%d",&n);
    printf("Enter the element");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("\n negative element in the array is");
    for(i=0;i<n;i++)
    {
        if(arr[i]<0)
        {
            printf("%d",arr[i]);
        }
        printf(" ");
    }
    
}
