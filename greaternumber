    if(a>b)
    {
        if(a>c)
        {
        printf("A is greater");
        }
        else
        {
            if(c>b)
            {
                printf("c is greater ");
            }
            
            
        }
        
    }
    else 
    {
        if(b>c)
        {
        printf("B is greater");
        }
        else 
        {
            if(c>a)
            {
                printf("c is greater");
            }
        }
