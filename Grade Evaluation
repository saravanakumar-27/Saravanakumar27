#include<stdio.h>
void main()
{
   int t;
   printf("enter the Mark ?");
   scanf("%d",&t);
   
   if(t>=90)
   printf("Grade A");
   else if(t>=80&&t<=89)
   printf("Grade B");
   else if(t>=70&&t<=79)
   printf("Grade c");
   else if(t>=60&&t<=69)
   printf("Grade D");
   else
   printf("Grade F");
}
