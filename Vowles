#include<stdio.h>
void main()
{
   char ch;
   scanf("%c",&ch);
   printf("Enter the charcter");
   if(ch=='a' || ch=='e' ||ch=='i' ||ch=='o' ||ch=='u' ||ch=='A' ||ch=='E' 
   ||ch=='I' ||ch=='O' ||ch=='U')
   {
      printf("The Character %c is a vowel.\n",ch);
   }
   else
   {
      printf("The Character %c is a consonants.\n",ch); 
   }
   
}

