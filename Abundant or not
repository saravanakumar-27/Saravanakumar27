#include<stdio.h>
void main()
{
 int i,j,n,sum=0;
 printf(" Enter the number : ");
 scanf("%d",&n);
 for(i=1;i<n;i++)
 {
    if(n%i==0)
    {
        sum=sum+i;
    }
 }
  if(sum>n)
  {
      printf(" %d is Abundant number\n",n);
      printf(" Number : %d\n Sum : %d\n Abundance : %d",n,sum,(sum-n));
  }
  else
  {
      printf(" %d is not an Abundant number\n",n);
  }
}
